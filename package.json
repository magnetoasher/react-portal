{
  "private": true,
  "name": "portal",
  "version": "4.0.4",
  "scripts": {
    "clean": "rimraf .next node_modules",
    "secrets": "cat /dev/urandom | head -c 512 | openssl sha256 | cut -d ' ' -f 2",
    "jwt:cert": "openssl req -x509 -newkey rsa:4096 -keyout jwt.private.pem -out jwt.public.pem -nodes -days 3650 -subj '/CN=localhost'",
    "db:create": "db-migrate db:create portaldb",
    "db:down": "db-migrate down --verbose",
    "db:init": "db-migrate up --verbose",
    "dev": "nodemon --inspect server/main.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node .next/production-server/main.js",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --fix \"{lib,pages,server}/**/*.{js,jsx,ts,tsx}\"",
    "storybook": "start-storybook",
    "test": "jest"
  },
  "browserslist": [
    "last 3 version",
    "> .5%",
    "not dead"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn version --patch --no-git-tag-version && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },

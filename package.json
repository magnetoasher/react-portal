{
  "private": true,
  "name": "portal",
  "version": "4.2.41",
  "scripts": {
    "clean": "rimraf .next .nest",
    "secrets": "cat /dev/urandom | head -c 512 | openssl sha256 | cut -d ' ' -f 2",
    "jwt:cert": "(! test -f jwt.public.pem || ! test -f jwt.private.pem) && openssl req -x509 -newkey rsa:4096 -keyout jwt.private.pem -out jwt.public.pem -nodes -days 3650 -subj '/CN=localhost' && echo 'JWT deployed !' || echo 'JWT already placed...'",
    "dev": "nodemon --inspect=0.0.0.0 server/main.ts",
    "build": "yarn clean && yarn nest:build && yarn copy && yarn next:build",
    "copy": "cp -r static .nest",
    "nest:build": "NODE_ENV=production tsc -P tsconfig.server.json",
    "next:build": "next build",
    "start": "NODE_ENV=production node .nest/main.js",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --fix \"{lib,pages,components,server}/**/*.{js,jsx,ts,tsx}\"",
    "storybook": "start-storybook",
    "test": "jest"
  },
  "browserslist": [
    "last 3 version",
    "> .5%",
    "not dead"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },

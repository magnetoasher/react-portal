{
  "name": "portal",
  "version": "4.7.0",
  "scripts": {
    "v": "yarn version --patch",
    "v:major": "yarn version --major",
    "v:minor": "yarn version --minor",
    "preversion": "yarn test -u",
    "postversion": "git push --tags",
    "clean": "rimraf dist .next apps/portal/.next/nest",
    "clean:node": "rimraf node_modules",
    "clean:yarn": "rimraf yarn.lock",
    "clean:all": "rimraf dist .next apps/portal/.next package.lock ~/.npm ~/.node-gyp ~/.cache/node-gyp ~/.cache/yarn ~/.config/yarn ~/.yarn yarn.lock node_modules",
    "clean:build": "source .local/env && yarn && yarn build && ./entrypoint.sh test -u && yarn clean:node && yarn install --production && ./entrypoint.sh start",
    "secrets": "cat /dev/urandom | head -c 512 | openssl sha256 | cut -d ' ' -f 2",
    "dev": "nest start -d -w",
    "dev:sync": "NODE_OPTIONS=--max_old_space_size=4096 nest start sync -w -d :9230",
    "dev:syncJob": "NODE_OPTIONS=--max_old_space_size=4096 nest start sync-job -w -d :9231",
    "build": "yarn clean && yarn build:next && yarn copy && yarn build:nest",
    "build:sync": "yarn build:nest sync",
    "build:syncJob": "yarn build:nest sync-job",
    "build:nest": "NODE_ENV=production nest build",
    "build:next": "cd apps/portal && NODE_ENV=production next build",
    "copy": "cp -r apps/portal/.next .next && rm -rf .next/cache",
    "helm": "helm upgrade --install --namespace=production --dry-run --debug --values helm/portal/values.yaml --set NODE_ENV=\"${NODE_ENV}\" --set PORT=\"${PORT}\" --set DATABASE_URI=\"${DATABASE_URI}\" --set DATABASE_URI_RD=\"${DATABASE_URI_RD}\" --set DATABASE_SCHEMA=\"${DATABASE_SCHEMA}\" --set DATABASE_SYNCHRONIZE=\"${DATABASE_SYNCHRONIZE}\" --set DATABASE_DROP_SCHEMA=\"${DATABASE_DROP_SCHEMA}\" --set DATABASE_MIGRATIONS_RUN=\"${DATABASE_MIGRATIONS_RUN}\" --set DATABASE_LOGGING=\"${DATABASE_LOGGING}\" --set DATABASE_REDIS_URI=\"${DATABASE_REDIS_URI}\" --set DATABASE_REDIS_TTL=\"${DATABASE_REDIS_TTL}\" --set HTTP_REDIS_URI=\"${HTTP_REDIS_URI}\" --set HTTP_REDIS_TTL=\"${HTTP_REDIS_TTL}\" --set HTTP_REDIS_MAX_OBJECTS=\"${HTTP_REDIS_MAX_OBJECTS}\" --set SESSION_REDIS_URI=\"${SESSION_REDIS_URI}\" --set SESSION_COOKIE_TTL=\"${SESSION_COOKIE_TTL}\" --set SESSION_SECRET=\"${SESSION_SECRET}\" --set LDAP_URL=\"${LDAP_URL}\" --set LDAP_BIND_DN=\"${LDAP_BIND_DN}\" --set LDAP_BIND_PW=\"${LDAP_BIND_PW}\" --set LDAP_SEARCH_BASE=\"${LDAP_SEARCH_BASE}\" --set LDAP_SEARCH_FILTER=\"${LDAP_SEARCH_FILTER}\" --set LDAP_SEARCH_GROUP=\"${LDAP_SEARCH_GROUP}\" --set LDAP_SEARCH_BASE_ALL_USERS=\"${LDAP_SEARCH_BASE_ALL_USERS}\" --set LDAP_SEARCH_FILTER_ALL_USERS=\"${LDAP_SEARCH_FILTER_ALL_USERS}\" --set LDAP_REDIS_URI=\"${LDAP_REDIS_URI}\" --set LDAP_REDIS_TTL=\"${LDAP_REDIS_TTL}\" --set MICROSERVICE_URL=\"${MICROSERVICE_URL}\" --set SOAP_URL=\"${SOAP_URL}\" --set SOAP_USER=\"${SOAP_USER}\" --set SOAP_PASS=\"${SOAP_PASS}\" --set NEWS_URL=\"${NEWS_URL}\" --set NEWS_API_URL=\"${NEWS_API_URL}\" --set MAIL_URL=\"${MAIL_URL}\" --set MAIL_LOGIN_URL=\"${MAIL_LOGIN_URL}\" --set MEETING_URL=\"${MEETING_URL}\" --set image.repository=\"${DOCKER_REGISTRY}/${RELEASE_NAME}\" --set image.tag=\"${CI_BUILD_REF}\" portal helm/portal",
    "helm:del": "helm uninstall --namespace=production portal",
    "docker": "docker build --tag portal --file Dockerfile.portal . && docker tag portal:master portal:latest",
    "start": "NODE_ENV=production node --max_old_space_size=4096 .next/nest/main.js",
    "start:sync": "NODE_ENV=production node --max_old_space_size=4096 dist/apps/sync/main.js",
    "start:syncJob": "NODE_ENV=production node --max_old_space_size=4096 dist/apps/sync-job/main.js",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --fix \"{lib,pages,components,src}/**/*.{js,jsx,ts,tsx}\"",
    "storybook": "start-storybook",
    "cli:login": "curl -X POST -H 'content-type: application/json' -d '{\"query\":\"mutation{login(username:\\\"'${LOGIN}'\\\",password:\\\"'${PASSWORD}'\\\"){token}}\"}' ${HTTP:-http}://${HOST:-localhost}:${PORT:-4000}/graphql",
    "cli:sync": "curl -X POST -H 'content-type: application/json' -H 'Authorization: bearer '${TOKEN} -d '{\"query\":\"mutation { synchronization }\"}' ${HTTP:-http}://${HOST:-localhost}:${PORT:-4000}/graphql",
    "test": "jest",
    "test:next": "jest --watch __tests__",
    "coverage": "yarn test -u --coverage",
    "typeorm": "ts-node -P ./tsconfig.ormconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:clean": "yarn typeorm schema:drop && yarn typeorm schema:sync",
    "typeorm:sync": "yarn typeorm schema:sync"
  },
  "browserslist": [
    "last 2 version",
    "> 2%",
    "not dead",
    "node >= 13",
    "edge > 14",
    "not ie 11",
    "not ie_mob 11",
    "not op_mini all"
